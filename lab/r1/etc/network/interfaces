auto lo
iface lo inet loopback

auto eth0
iface eth0 inet static
	address 1.0.0.1
	netmask 255.255.255.0

auto eth1
iface eth1 inet static
        address 10.0.2.1
        netmask 255.255.255.0

auto eth2
iface eth2 inet static
        address 10.0.1.1
        netmask 255.255.255.0

auto eth3
iface eth3 inet static
        address 10.0.0.1
        netmask 255.255.255.0

auto eth5
iface eth5 inet static
        address 1.1.255.13
        netmask 255.255.255.254
